{
  "name": "vue-layouter",
  "version": "0.0.1-alpha.8",
  "private": false,
  "main": "dist/vue-layouter.common.js",
  "module": "dist/vue-layouter.esm.js",
  "unpkg": "dist/vue-layouter.js",
  "typings": "dist/types/index.d.ts",
  "license": "MIT",
  "author": "wivi1995",
  "keywords": [
    "vue",
    "layout",
    "layouter"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:esm": "rollup --config rollup.config.js",
    "build:umd": "rollup --config rollup.config.js --format umd",
    "build:cjs": "rollup --config rollup.config.js --format cjs",
    "build:types": "tsc --build tsconfigtypes.json",
    "build": "rimraf dist && npm run build:esm && npm run build:umd && npm run build:cjs && npm run build:types",
    "release:major": "standard-version -r major",
    "release:minor": "standard-version -r minor",
    "release:patch": "standard-version -r patch",
    "release:alpha": "standard-version --prerelease alpha",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wivi1995/vue-layouter.git"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue-router": ">=3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.1.0",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-plugin-transform-require-context": "^0.1.1",
    "commitizen": "^4.0.3",
    "core-js": "^3.6.5",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.0.6",
    "lint-staged": "^9.5.0",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "standard-version": "^9.3.2",
    "typescript": "~4.1.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test:unit"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run test:unit && npm run build"
    }
  }
}
